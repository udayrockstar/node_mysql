html
	head
		title!= id
		link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css')
	body
		//h1!= name
		//each kw in rows
		//	li= kw.name
		//h2(id="uday") Uday Hosamani
		
		div(class="container")
			div(class="row")
				form(action="/save",method="post")
					div(class="col-sm-6")
						div(class="alert alert-success",role="alert") Add Form
						label Name :
						input(type="text",name="name",id="name",class="form-control")
						label Last :
						input(type="text",name="last",id="last",class="form-control")
						br
						button(class="btn btn-danger", type="submit", data-action="bea").Be Awesome
			
			div(class="row")
				table(class="table table-striped")
					thead
						tr
							th Name
							th Last
					tbody
					each kw in rows
						tr
							td  kw.name
							td Hosamani
					